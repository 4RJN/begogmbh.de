---
layout: null
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- assign canonical_base = site.canonical_base | default: site.url | append: site.baseurl -%}
  {%- for p in site.pages -%}
    {%- if p.url == '/404.html' -%}{%- continue -%}{%- endif -%}
    {%- if p.url contains '/_' -%}{%- continue -%}{%- endif -%}
    {%- if p.name == 'sitemap.xml' -%}{%- continue -%}{%- endif -%}
    {%- if p.url == '/' or p.url contains '.html' -%}
  <url>
    <loc>{{ canonical_base }}{{ p.url | replace: 'index.html','' }}</loc>
  </url>
    {%- endif -%}
  {%- endfor -%}
</urlset>
